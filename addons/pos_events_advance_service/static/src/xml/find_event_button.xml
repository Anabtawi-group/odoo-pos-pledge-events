<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
  <t t-inherit="point_of_sale.ControlButtons" t-inherit-mode="extension">
    <xpath expr="//div[contains(@class,'control-buttons')]" position="inside">
      <button class="control-button" t-on-click="onClickFindEvent">
        <i class="fa fa-search" role="img"/>
        <span>Find Event</span>
      </button>
    </xpath>
  </t>
</templates>
